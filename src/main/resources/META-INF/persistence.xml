<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0"
			 xmlns="http://java.sun.com/xml/ns/persistence"
			 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">

	<persistence-unit name="pu">
		<provider>com.impetus.kundera.KunderaPersistence</provider>
		<class>it.polimi.kundera.entities.Department</class>
		<class>it.polimi.kundera.entities.Employee</class>
		<class>it.polimi.kundera.entities.EmployeeMTM</class>
		<class>it.polimi.kundera.entities.EmployeeMTO</class>
		<class>it.polimi.kundera.entities.EmployeeOTO</class>
		<class>it.polimi.kundera.entities.Phone</class>
		<class>it.polimi.kundera.entities.ProjectMTM</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="kundera.keyspace" value="gae"/>
			<property name="kundera.client.lookup.class" value="it.polimi.kundera.client.datastore.DatastoreClientFactory"/>
		</properties>
	</persistence-unit>

</persistence>